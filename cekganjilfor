#include <iostream>

using namespace std;

int main() {
   int arr[100];
   int n, total=0, i;
   cout << "Masukkan jumlah bilangan : ";
   cin >> n;
   for(i=0; i<n; i++){
       cout << "Masukkan bilangan ke " << i+1 << " : ";
       cin >> arr[i];
   }
   for(i=0; i<n; i++){
       if(arr[i]%2!=0){
           total = total+arr[i];
       }
   }
   cout << "totalnya bilangan ganjil adalah : " << total;
   
}
